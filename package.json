{
  "name": "react-ssr",
  "version": "0.10.1",
  "description": "The simplest Async Universal React & Redux boilerplate.",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev-server": "cross-env BABEL_ENV=server NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev.js",
    "dev": "concurrently \"cross-env NODE_ENV=development npm run dev-server\" \"cross-env NODE_ENV=development npm run dev-client\"",
    "prod-server": "cross-env BABEL_ENV=server NODE_ENV=production babel src --out-dir ./dist",
    "prod-client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write ."
  },
  "author": "Komal Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/komalsingh54/react-ssr"
  },
  "homepage": "https://github.com/komalsingh54/react-ssr",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "express",
    "react-router"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.11.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
